{
  "version": 2,
  "builds": [
    {
      "src": "**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/ask-eco",
      "dest": "/api/ask-eco.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    }
  ]
}